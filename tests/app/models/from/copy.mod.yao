{
  "LANG": "1.0.0",
  "VERSION": "1.0.0",
  "FROM": "@github.com/yaoapp/workshop-tests-cms/models/article",
  "columns": [
    {
      "COPY": "user.phone",
      "name": "phone_no",
      "validations": [
        {
          "method": "typeof",
          "args": ["string"],
          "message": "Copy from user {{input}} should be string"
        }
      ],
      "comment": "User Phone number"
    },
    {
      "COPY": "dict/product.title",
      "name": "product_title",
      "comment": "The product title",
      "validations": [
        {
          "method": "typeof",
          "args": ["string"],
          "message": "Copy from dict/product {{input}} should be string"
        }
      ]
    }
  ]
}
