{
  "LANG": "1.0.0",
  "VERSION": "1.0.0",
  "FROM": "@github.com/yaoapp/workshop-tests-cms/models/article",
  "RUN": {
    "REPLACE": [{ "table.name": "$env.GOU_TEST_DSL_TABLE_NAME" }]
  },
  "columns": [
    {
      "COPY": "user.phone",
      "name": "phone_no",
      "validations": [
        {
          "method": "typeof",
          "args": ["string"],
          "message": "ENV NAME user {{input}} should be string"
        }
      ],
      "comment": "$env.GOU_TEST_DSL_TABLE_NAME"
    },
    {
      "COPY": "dict/product.title",
      "name": "product_title",
      "comment": "$env.GOU_TEST_DSL_TABLE_NAME",
      "validations": [
        {
          "method": "typeof",
          "args": ["string"],
          "message": "Copy from dict/product {{input}} should be string"
        }
      ]
    }
  ]
}
